#include<iostream>
using namespace std;
int compareTriplet(int alice[], int bob[],int n)
{ int ali=0,bot=0;
    for(int i=0;i<n;i++)
    {
        if(alice[i]>bob[i])
        ali++;
        else if (alice[i]<bob[i])
        bot++; 
        else if (alice[i]==bob[i])          
        {
            ali;
            bob;
        }
        
          
    }
    cout<<ali<<" "<<bot;
    return 0;
}
int main()
{   int n;
    cin>>n;
    int alice[n];
    int bob[n];
    for(int i=0;i<n;i++)
    {
        cin>>alice[i];
    }
    for(int i=0;i<n;i++)
    {
        cin>>bob[i];
    }
    compareTriplet(alice,bob,n);
    return 0;
}
