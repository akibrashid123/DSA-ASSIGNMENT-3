#include<iostream>
using namespace std;
void simpleArraysum(int arr[],int n)
{
int sum;
    for(int i=0; i<n; i++)
    {
        sum= sum +arr[i];
    }
    cout<< sum;
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    
    int sum;
    simpleArraysum(arr,n);
    
    return 0;
}
